{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "build/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/login",
        "dest": "/app.py"
      },
      {
        "src": "/callback",
        "dest": "/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      }
    ]
  }